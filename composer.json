{
	"name": "pressmodo/wp-simple-menu-icons",
	"license": "",
	"description": "An easy way to add icons to your navigation menus.",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress"
	],
	"homepage": "https://pressmodo.com",
	"authors": [{
			"name": "Alessandro Tesoro",
			"email": "hello@sematico.com",
			"homepage": "https://alessandrotesoro.me",
			"role": "Developer"
		},
		{
			"name": "Sematico LTD",
			"email": "hello@sematico.com",
			"homepage": "https://sematico",
			"role": "Founder & MD"
		}
	],
	"require": {
		"php": ">=5.6",
		"composer/installers": "~1.0",
		"pressmodo/wp-requirements": "^1.0",
		"symfony/yaml": "^5.1"
	},
	"autoload": {
		"psr-4": {
			"Pressmodo\\MenuIcons\\": "src/"
		}
	},
	"require-dev": {
		"pressmodo/phpcs-config": "dev-master",
		"phpunit/phpunit": "^9.0.1",
		"php-mock/php-mock-phpunit": "^2.5",
		"10up/wp_mock": "dev-master",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
	},
	"scripts": {
		"lint": "phpcs .",
		"lint-fix": "phpcbf .",
		"phpunit": "phpunit",
		"phpunit-coverage": "phpunit --coverage-text"
	}
}
